                <span class="ps-3 d-none d-lg-block">
                    <a href="tel:+11234567890" class="btn btn-primary" target="_blank"><i class="las la-phone"></i> +905511378777</a>
                </span>
                <span class="ps-3 d-none d-xl-block">
                    <a href="https://fatihmehmetdeveci.com.tr"
                       class="btn btn-primary" target="_blank"><i class="las la-sms"></i></a>
                </span>
            </div>
        </div>

    <!-- NAVIGATION ENDS
            ========================================================================= -->
    <!-- SLIDER STARTS
            ========================================================================= -->
    <section id="home">
        <div id="slider">
            <div id="rev_slider_24_1_wrapper" class="rev_slider_wrapper fullscreen-container" data-alias="website-intro"
                 data-source="gallery" style="background:#000000;padding:0px;">

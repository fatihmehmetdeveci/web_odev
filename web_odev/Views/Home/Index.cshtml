                <div id="rev_slider_24_1" class="rev_slider fullscreenbanner tiny_bullet_slider" style="display:none;"
                     data-version="5.4.1">
                    <ul>
                        <!-- SLIDE  -->
                        @foreach (var item in Model)
                        {
                            <li data-index="rs-@item.id" data-transition="zoomout" data-slotamount="default"
